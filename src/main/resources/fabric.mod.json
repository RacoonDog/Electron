{
  "schemaVersion": 1,
  "id": "electron",
  "version": "${version}",
  "name": "Electron",
  "description": "Experimental performance improvements for Meteor Client.",
  "authors": [ "Crosby" ],
  "contact": {
    "issues": "https://github.com/RacoonDog/Electron/issues",
    "sources": "https://github.com/RacoonDog/Electron"
  },
  "license": "ARR",
  "icon": "assets/electron/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "io.github.racoondog.electron.Electron"
    ]
  },
  "mixins": [
    "electron.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "255,201,58",
    "meteor-client:commit": "${commit}"
  },
  "depends": {
    "java": ">=21",
    "fabricloader": ">=0.15",
    "minecraft": "~1.21",
    "meteor-client": "*"
  }
}
