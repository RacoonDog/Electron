{
  "schemaVersion": 1,
  "id": "electron",
  "version": "${version}",
  "name": "Electron",
  "description": "Meteor Client Performance Optimizations.",
  "authors": [ "Crosby" ],
  "contact": {
    "repo": "https://github.com/RacoonDog/Electron",
    "issues": "https://github.com/RacoonDog/Electron/issues",
    "sources": "https://github.com/RacoonDog/Electron"
  },
  "license": "ARR",
  "icon": "assets/electron/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "io.github.racoondog.electron.Electron"
    ],
    "preLaunch": [
      "io.github.racoondog.electron.Electron"
    ]
  },
  "mixins": [
    "electron.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "255,201,58",
    "modmenu": {
      "badges": [ "library", "meteor" ]
    }
  },
  "depends": {
    "java": ">=17",
    "fabricloader": ">=0.12",
    "minecraft": ">=${mc_version}",
    "meteor-shared-addon-utils": "*",
    "meteor-client": "*"
  }
}
